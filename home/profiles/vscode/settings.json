{
  "workbench.iconTheme": "material-icon-theme",
  "redhat.telemetry.enabled": false,
  "gitlens.currentLine.uncommittedChangesFormat": "✏️ ${ago}",
  "gitlens.codeLens.enabled": false,
  "gitlens.hovers.enabled": false,
  "gitlens.showWelcomeOnInstall": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "editor.fontFamily": "'Iosevka Xtal', Iosevka, 'Fira Code', 'JetBrains Mono', Menlo, Monaco, 'Courier New', monospace",
  "editor.tabSize": 2,
  "editor.codeLensFontFamily": "'Iosevka Xtal Term', 'Iosevka Term'",
  "editor.linkedEditing": true,
  "editor.parameterHints.cycle": true,
  "editor.rulers": [80, 100],
  "editor.cursorSmoothCaretAnimation": true,
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.formatOnSave": true,
  "editor.minimap.autohide": true,
  "files.autoGuessEncoding": true,
  "files.insertFinalNewline": true,
  "workbench.preferredDarkColorTheme": "Vitesse Dark",
  "workbench.preferredHighContrastColorTheme": "GitHub Dark High Contrast",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.excludeDirty": true,
  "workbench.editor.limit.perEditorGroup": true,
  "window.autoDetectColorScheme": true,
  "workbench.preferredHighContrastLightColorTheme": "GitHub Light High Contrast",
  "workbench.preferredLightColorTheme": "GitHub Light Monochrome Focused",
  "window.restoreFullscreen": true,
  "explorer.excludeGitIgnore": true,
  "search.exclude": {
    "**/vendor": true
  },
  "search.smartCase": true,
  "search.useGlobalIgnoreFiles": true,
  "terminal.external.osxExec": "~/Applications/kitty.app",
  "terminal.integrated.cursorBlinking": true,
  "github.gitProtocol": "ssh",
  "nix.enableLanguageServer": true,
  "nix.serverPath": "/etc/profiles/per-user/cdom/bin/rnix-lsp",
  "vscode-neovim.neovimExecutablePaths.darwin": "/etc/profiles/per-user/cdom/bin/nvim",
  "vscode-neovim.neovimExecutablePaths.linux": "/etc/profiles/per-user/cdom/bin/nvim",
  "vscode-neovim.mouseSelectionStartVisualMode": true,
  "extensions.closeExtensionDetailsOnViewChange": true,
  "markdown.preview.fontFamily": "'IBM Plex Sans', -apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', system-ui, 'Ubuntu', 'Droid Sans', sans-serif",
  "git.autoStash": true,
  "git.closeDiffOnOperation": true,
  "git.defaultCloneDirectory": "~/Developer/scratch",
  "git.enableCommitSigning": true,
  "git.fetchOnPull": true,
  "git.scanRepositories": [
    "~/Developer/work",
    "~/Developer/sources",
    "~/Developer/contrib"
  ],
  "git.smartCommitChanges": "tracked",
  "git.suggestSmartCommit": false,
  "git.repositoryScanMaxDepth": 2,
  "gitlens.blame.ignoreWhitespace": true,
  "editor.fontWeight": "normal",
  "diffEditor.renderSideBySide": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "css.format.spaceAroundSelectorSeparator": true,
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "eslint.alwaysShowStatus": true,
  "scm.diffDecorations": "gutter",
  "scm.inputFontFamily": "editor",
  "terminal.integrated.fontFamily": "'Iosevka Xtal Term', 'Iosevka Term'",
  "vim.cursorStylePerMode.insert": "line",
  "vim.cursorStylePerMode.normal": "block",
  "vim.cursorStylePerMode.replace": "underline",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": true,
    "<C-d>": true,
    "<C-s>": false,
    "<C-z>": false
  },
  "vsicons.dontShowNewVersionMessage": true,
  "workbench.fontAliasing": "auto",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "magit.forge-enabled": true,
  "vim.gdefault": true,
  "vim.highlightedyank.enable": true,
  "vim.history": 300,
  "vim.hlsearch": true,
  "vim.leader": "<space>",
  "vim.mouseSelectionGoesIntoVisualMode": false,
  "vim.showMarksInGutter": true,
  "vim.smartRelativeLine": true,
  "vim.sneak": true,
  "vim.sneakReplacesF": true,
  "vim.useSystemClipboard": true,
  "vim.visualstar": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "colorize.languages": [
    "javascript",
    "css",
    "sass",
    "scss",
    "less",
    "postcss",
    "sss",
    "stylus",
    "xml",
    "svg"
  ],
  "files.associations": {
    "dep5": "debian-copyright"
  },
  "terminal.integrated.cursorStyle": "line",
  "workbench.statusBar.visible": false,
  "editor.minimap.enabled": false,
  "editor.renderLineHighlightOnlyWhenFocus": true,
  "editor.occurrencesHighlight": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "tailwindCSS.emmetCompletions": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.run": "onSave",
  "workbench.colorTheme": "Vitesse Dark",
  "editor.definitionLinkOpensInPeek": true,
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "material-icon-theme.saturation": 0.5,
  "workbench.activityBar.visible": false,
  "workbench.editor.enablePreviewFromCodeNavigation": true,
  "workbench.editor.enablePreviewFromQuickOpen": true,
  "workbench.editor.showTabs": false,
  "workbench.editor.preferHistoryBasedLanguageDetection": true,
  "workbench.editor.pinnedTabSizing": "shrink",
  "workbench.editor.highlightModifiedTabs": true,
  "zenMode.fullScreen": false,
  "window.dialogStyle": "custom",
  "problems.showCurrentInStatus": true,
  "workbench.layoutControl.enabled": false,
  "hoverlens.debounceUpdate": 1000,
  "errorLens.fontFamily": "'Iosevka Xtal Term'",
  "errorLens.fontStyleItalic": true,
  "errorLens.gutterIconsEnabled": true,
  "errorLens.messageTemplate": "$message [$source:$code]",
  "errorLens.enabledInMergeConflict": false
}
